{
  "model": {
      "output_dir": "output/test",
      "pad_mode": "constant",
      "filters": [16, 16, 32, 64],
      "pad_size": 32
  },

  "training": {
      "tf_random_seed": 557212,
      "np_random_seed": 993101,
      "max_num_training_steps": 6000,
      "num_output_steps": 80,
      "num_summary_steps": 80,
      "num_eval_steps": 80,
      "num_checkpoint_steps": 80,
      "batch_size": 64,
      "step_size_schedule": [[0, 0.1], [400, 0.01], [600, 0.001]],
      "momentum": 0.9,
      "weight_decay": 0.0002,
      "eval_during_training": true,
      "adversarial_training": false,
      "data_augmentation": true
  },

  "eval": {
      "num_eval_examples": 2000,
      "batch_size": 64,
      "adversarial_eval": true
  },

  "attack": {
      "use_linf": false,
      "use_spatial": true,
      "attack_method": "none",

      "loss_function": "xent",
      "epsilon": 8.0,
      "num_steps": 5,
      "step_size": 2.0,
      "random_start": false,

      "spatial_method": "random",
      "spatial_limits": [0, 0, 0],
      "random_tries": 10,
      "grid_granularity": [5, 5, 31]
  },

  "data": { "data_path": "images-greebles-inv-tt" }
}
